{
  "name": "HackerNews for Power Users",
  "description": "browse HN without using the mouse",
  "version": "0.1",
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png" },
  "content_scripts": [
    {
      "matches": ["*://news.ycombinator.com/", "*://news.ycombinator.com/news"],
      "js": ["index.js"]
    },
    {
      "matches": ["*://news.ycombinator.com/*"],
      "css": ["styles.css"]
    }
  ],
  "permissions": [
    "storage"
  ],

  "manifest_version": 2
}
